@model IEnumerable<SchoolManagement.Models.ViewModels.StudentUserDetails>
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Result Corner";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="~/css/Index.css" />
</head>
<body>
    <div class="text-sm-right">
        <h2 class="btn-block">Welcome @User.FindFirst("names").Value  </h2>
    </div>
    <div class="col-2 text-right" style="float:right">
        <img src="\Images\@User.FindFirst("image").Value" width="30%" style="border-radius:5px; border:1px solid #4cff00">
    </div>
    <div class="col-2 text-right" style="float:right">
        <a type="submit" class="btn btn-primary form-control" asp-area="Users" asp-controller="Login" asp-action="Logout">Logout</a>
    </div>
    <div class="text-sm-left">
        <h2 class="btn-block">Create Result</h2>
    </div>
    <table id="tblData" class="table table-striped table-bordered" style="width:100%">
        <thead>
        <tr class="table-info">
            <th>UserId</th>
            <th>Name</th>
            <th>Email</th>
            <th>Class</th>
            <th></th>
            <th></th>
        </tr>
       </thead>
        </table>
</body>
</html>
@section Scripts{
    <script src="~/js/result.js"></script>
}



